{
    "env": {
        "browser": true,
        "node": true
    },
    "ecmaFeatures": {
        "arrowFunctions": true,
        "spread": true,
        "classes": true,
        "destructuring": true,
        "jsx": true
    },
    "rules": {
        "dot-notation": 1,
        "eol-last": 2,
        "max-depth": [1, 6],
        "max-len": [2, 120],
        "no-alert": [1, false],
        "no-mixed-requires": [0, false],
        "max-statements": [1, 100],
        "new-cap": 1,
        "no-lonely-if": 2,
        "no-multiple-empty-lines": 2,
        "no-nested-ternary": 2,
        "no-spaced-func": [1, true],
        "no-underscore-dangle": [0, false],
        "no-unused-expressions": [0, false],
        "no-trailing-spaces": 2,
        "no-use-before-define": [1, "nofunc"],
        "quotes": [2, "single"],
        "semi": [2, "always"],
        "space-after-keywords": [2, "always"],
        "strict": 0
    }
}
